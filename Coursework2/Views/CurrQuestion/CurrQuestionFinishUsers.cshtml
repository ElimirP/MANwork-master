@model Tuple<string, List<Coursework2.Data.Models.ResultsTable>>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Lubota's MAN</title>
</head>
<body>
    <table>
        <tr>
            <td>Ім'я учасника</td>
            <td>Кількість балів</td>
        </tr>
        
            @{
                foreach (var x in Model.Item2)
                {
                    <tr>
                        <td>@x.NameUsers</td>
                        <td>@x.CountPoints</td>
                    </tr>
                }
            }
        
    </table>

    <form method="post" action="/Users/ListUsersString">
        <input type="hidden" name="id" value="@Model.Item1">
        <button type="submit">Повернутися до списку игор</button>
    </form>
</body>
</html>
